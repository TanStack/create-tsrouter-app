  "license": "MIT",
  "packageManager": "pnpm@9.15.5",
  "dependencies": {
    "@clack/prompts": "^0.10.0",
    "@modelcontextprotocol/sdk": "^1.6.0",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "ejs": "^3.1.10",
    "execa": "^9.5.2",
    "express": "^4.21.2",
    "prettier": "^3.5.0",
    "vitest": "^3.0.8",
    "zod": "^3.24.2"
  },
